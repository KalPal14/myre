generator client {
  provider = "prisma-client-js"
  output   = "client"
}

datasource db {
  provider = "postgresql"
  url      = env("IAM_DB_URL")
}

model UserModel {
  id                Int       @id @default(autoincrement())
  username          String    @unique
  email             String    @unique
  password          String
  passwordUpdatedAt DateTime?
}
